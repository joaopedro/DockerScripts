	<action-mappings type="org.apache.struts.action.ActionMapping">

		<!-- PRIVATE Action to unlock a user. Uses the service UserAuthenticationManager -->
		<action path="/admin/unlockUser"
			type="com.sinfic.ipdms.administration.action.authentication.UnlockColaboradorAction">
			<forward name="ok" path="/editColaborador.do2" />
		</action>
		
		<!-- PUBLIC Action to unlock a user. Uses the service UserAuthenticationManager -->
		<action path="/public/unlockUserByCode"
			type="com.sinfic.ipdms.administration.action.authentication.UnlockColaboradorByCodeAction">
			<forward name="ok" path="ipdms.colaborador.unlock.unlocked" />
			<forward name="error" path="ipdms.colaborador.unlock.error" />
		</action>

		<!-- PUBLIC Action - Form to send the e-mail with the unlock code -->
		<action path="/public/unlockUserForm"
			type="com.sinfic.ipdms.comum.action.RedirectAction"
			name="lockUserForm" validate="false" scope="request">
			<forward name="default" path="ipdms.user.lock.form" />
		</action>
		
		<!-- PUBLIC Action - Send the e-mail with the unlock code -->
		<action path="/public/getUnlockCodeForUser"
			type="com.sinfic.ipdms.administration.action.authentication.GetUnlockCodeAction"
			name="lockUserForm" validate="true" input="ipdms.user.lock.form" scope="request">
			<forward name="invalid" path="ipdms.user.lock.unlockvaluesinvalid" />
			<forward name="ok" path="ipdms.user.lock.unlockcodesent" />
		</action>
	
